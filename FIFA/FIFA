
CREATE TABLE FIFA
(
ID VARCHAR(6),
Nome VARCHAR(255),
Idade INT,
"Data_Nascimento" DATE,
Posição VARCHAR(4),
Overall INT,
Pé_Bom VARCHAR(10),
Altura FLOAT,
Peso FLOAT,
Genero VARCHAR(10),
Chute INT,
Passe INT,
Drible INT,
Defesa INT,
Fisico INT,
Atributo_de_Goleiro INT,
Clube INT,
Liga VARCHAR(255),
Genero_Liga VARCHAR(255),
Nacionalidade VARCHAR(255),
IMC FLOAT,
Classificacao_IMC VARCHAR(255)
        );
